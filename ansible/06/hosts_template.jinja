[pfsense]
fw1 ansible_host={{ fw1_lan_ip }}

[pfsense:vars]
ansible_connection=ssh
ansible_user={{ pfsense_user }}
ansible_password={{ pfsense_pass }}
fw1_lan_ip={{ fw1_lan_ip }}

[wan_windows]
wan-win7 ansible_host={{ wan_win7_ip }}
wan-win10 ansible_host={{ wan_win10_ip }}

[windows:children]
wan_windows

[windows:vars]
ansible_connection=winrm
ansible_port=5985
ansible_user={{ windows_user }}
ansible_password={{ windows_password }}

[windows_server]
winserver ansible_host={{ winserver_ip }}

[windows_server:vars]
ansible_connection=winrm
ansible_port=5985
ansible_user={{ windows_server_user }}
ansible_password={{ windows_server_password }}